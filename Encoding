#include<iostream>
#include<string>
using namespace std;
int main()
{
	string str,t_str;
	int n,i;
	cin>>n;
	while(n--)
	{cin>>str;
	 t_str=str[0];
	 int temp=0;
	 for(i=0;i<str.size();i++)
	     {if(str[i]==t_str[0])
	          {temp++;
			   if(i==str.size()-1)
			        if(temp==1) cout<<t_str[0];
					else  cout<<temp<<t_str[0];
              }
          else 
              {if(temp==1)   cout<<t_str[0];
              else          cout<<temp<<t_str[0];
              t_str[0]=str[i];
              temp=1;
              if(i==str.size()-1)
                     if(temp==1) cout<<t_str[0];
                     else        cout<<temp<<t_str[0];
              }
       }
	 cout<<endl;
	 str="";
	}
}
